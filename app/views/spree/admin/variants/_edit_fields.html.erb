<h3><%= t("group_prices") %></h3>
<table class="index">
  <thead>
    <tr>
      <th><%= t("name") %></th>
      <th><%= t("discount_type") %></th>
      <th><%= t("range") %></th>
      <th><%= t("amount") %></th>
      <th><%= t("position") %></th>
      <th><%= t("action") %></th>
    </tr>
  </thead>
  <tbody id="group_prices">
    <%= f.fields_for :group_prices do |vp_form| %>
      <%= render 'group_price_fields', :f => vp_form %>
    <% end %>
  </tbody>
</table>
<%= link_to_add_fields t("add_group_price"), "tbody#group_prices" %>
<br/><br/>
